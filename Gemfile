source 'https://rubygems.org'

# Bundle edge Rails instead: gem 'rails', github: 'rails/rails'
gem 'rails'
gem 'certified'

group :development do
	gem 'sqlite3'
end

# Gems used only for assets and not required
# in production environments by default.
group :assets do
	gem 'sass-rails'
	gem 'coffee-rails'
	gem 'uglifier'
end

gem 'jbuilder'
gem 'turbolinks'
gem 'jquery-rails'
gem 'execjs'

group :production do
	gem 'pg'
end